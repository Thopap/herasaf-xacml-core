<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false">
  <fileset name="all" enabled="false" check-config-name="herasaf-reporting" local="false">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <fileset name="herasaf-checkstyle-maven-fileset" enabled="true" check-config-name="herasaf-reporting" local="false">
    <file-match-pattern match-pattern=".java$" include-pattern="false"/>
    <file-match-pattern match-pattern="src/main/java/" include-pattern="true"/>
  </fileset>
</fileset-config>
